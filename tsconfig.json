{
  "extends": "@tsconfig/node12/tsconfig.json",
  "compilerOptions": {
    "importHelpers": true,
    "moduleResolution": "node",
    "lib": ["DOM", "es2019", "es2020.promise", "es2020.bigint", "es2020.string"],
    "outDir": "./src",
    "baseUrl": ".",
    "paths": {
      "@typings/*": ["typings/*"],
      "@architect/shared/*": ["ts/shared/*"]
    }
  },
  "include": ["./typings", "./ts"],
  "exclude": ["node_modules"]
}
